    {
      "type": "terminal",
      "key": " ",
      "keyColor": "blue"
    },
    {
      "type": "shell",
      "key": " ",
      "keyColor": "blue"
    },
    {
      "type": "terminalfont",
      "key": "󰛖 ",
      "keyColor": "blue"
    },
